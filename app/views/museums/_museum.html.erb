<div id="<%= dom_id museum %>">
  <div class="card bg-base-100 w-96 shadow-xl">
    <figure>
    </figure>
    <div class="card-body">
      <h2 class="card-title">Review</h2>
      <p>展示会:<%= museum.name %></p>
      <p>住　所:<%= museum.location %></p>
      <p>営業時間:<%= museum.business_hours %></p>
      <p>入館料:<%= museum.admission_fees %></p>
      <p>URL:<%= museum.website_url %></p>
      <p>TEL:<%= museum.phone_number%></p>
      <%= link_to "Show this museum", museum, class:"btn btn-sm" %>
      <div class="card-actions justify-end">
        <div class="badge badge-outline">タグ１</div>
        <div class="badge badge-outline">タグ２</div>
      </div>
    </div>
  </div>
</div>
